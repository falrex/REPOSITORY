<h1><%# flash[:subjectname] %></h1>
<h2>enroll section</h2>

<table>
  <tr>
    <th>Section</th>
    <th>Time</th>
    <th>Day</th>
  </tr>


<% @sections.each do |section| %>
  <tr>
    <td><%=h section.section %></td>
    <td><%=h section.time %></td>
    <td><%=h section.day %></td>

    <td><%= link_to 'Enroll Section', :controller=>"enrollsubject", :action=>"enroll",:section=>section%></td>

  </tr>
<% end %>
</table>