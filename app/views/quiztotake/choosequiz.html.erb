<h1>Quizzes</h1>
<p><%= link_to 'Home', :controller=>"studentpage" %> -> Subjects</p>
<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
  </tr>

<% @quiz.each do |quiz| %>
